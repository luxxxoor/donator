                        <Insets top="20.0" />
                     </VBox.margin>
                  </Button>
               </children>
               <BorderPane.margin>
                  <Insets />
               </BorderPane.margin>
            </VBox>
         </center>
         <top>
            <Label text="Creare cont" BorderPane.alignment="CENTER">
               <BorderPane.margin>
                  <Insets top="70.0" />
               </BorderPane.margin>
               <font>
                  <Font name="Century Gothic" size="20.0" />
               </font>
            </Label>
         </top>
      </BorderPane>
