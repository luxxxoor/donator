<AnchorPane prefHeight="500.0" prefWidth="800.0" xmlns="http://javafx.com/javafx/9.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="controllers.PrevalidareCreareContController">
   <children>
      <BorderPane layoutX="200.0" layoutY="100.0" prefHeight="200.0" prefWidth="200.0" AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
         <center>
            <VBox alignment="CENTER" prefHeight="200.0" prefWidth="800.0" BorderPane.alignment="CENTER">
               <children>
                  <Label text="Înainte de a continua, te rugăm să specifici dacă ai suferit de:">
                     <font>
                        <Font size="14.0" />
                     </font>
                     <VBox.margin>
                        <Insets bottom="20.0" />
                     </VBox.margin>
                  </Label>
                  <GridPane alignment="CENTER" maxWidth="400.0" prefWidth="400.0">
                    <columnConstraints>
                      <ColumnConstraints halignment="LEFT" hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0" />
                      <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0" />
                    </columnConstraints>
                    <rowConstraints>
                      <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
                      <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
                      <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
                        <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
                        <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
                        <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
                        <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
                    </rowConstraints>
                     <children>
                        <CheckBox fx:id="hepatita" mnemonicParsing="false" text="Hepatită (de orice tip)" />
                        <CheckBox fx:id="bruceloza" mnemonicParsing="false" text="Bruceloză" GridPane.rowIndex="3" />
                        <CheckBox fx:id="diabetZaharat" mnemonicParsing="false" text="Diabet zaharat" GridPane.rowIndex="4" />
                        <CheckBox fx:id="sifilis" mnemonicParsing="false" text="Sifilis" GridPane.rowIndex="1" />
                        <CheckBox fx:id="tbc" mnemonicParsing="false" text="TBC" GridPane.columnIndex="1" />
                        <CheckBox fx:id="malarie" mnemonicParsing="false" text="Malarie" GridPane.columnIndex="1" GridPane.rowIndex="1" />
                        <CheckBox fx:id="epilepsie" mnemonicParsing="false" text="Epilepsie si alte boli neurologice" GridPane.rowIndex="2" />
                        <CheckBox fx:id="boliPsihice" mnemonicParsing="false" text="Boli psihice" GridPane.columnIndex="1" GridPane.rowIndex="2" />
                        <CheckBox fx:id="ulcer" mnemonicParsing="false" text="Ulcer" GridPane.columnIndex="1" GridPane.rowIndex="3" />
                        <CheckBox fx:id="boliDeInima" mnemonicParsing="false" text="Boli de inimă" GridPane.columnIndex="1" GridPane.rowIndex="4" />
                        <CheckBox fx:id="boliDePiele" mnemonicParsing="false" text="Boli de piele: psoriazis, vitiligo" GridPane.rowIndex="5" />
                        <CheckBox fx:id="miopieForte" mnemonicParsing="false" text="Miopie forte peste (-) 6 dioptrii" GridPane.columnIndex="1" GridPane.rowIndex="5" />
